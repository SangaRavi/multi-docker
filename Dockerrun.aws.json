{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions" : [
    {
     "name": "client",
     "image": "sangathamilan/multi-client",
     "hostname": "client",
     "essential": false
    },
     {
     "name": "nginx",
     "image": "sangathamilan/multi-nginx",
     "essential": true,
     "portMappings": [
      {
        "hostPort": 80,
        "containerPort": 80
      }
     ],
      "links": ["client", "server"]
    },
     {
     "name": "server",
     "image": "sangathamilan/multi-server",
     "hostname": "api",
     "essential": false
    },
     {
     "name": "worker",
     "image": "sangathamilan/multi-worker",
     "hostname": "worker",
     "essential": false
    }

  ]
}
